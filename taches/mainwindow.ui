     <property name="styleSheet">
      <string notr="true">QPushButton {
    background: none;
    background-color: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1, 
        stop:0 #2b2b2b,   /* Noir foncé */
        stop:0.5 #1e1e1e, /* Noir intermédiaire */
        stop:1 #000000    /* Noir profond */
    );
    color: white;              /* Texte en blanc */
    border: 2px solid #555;    /* Bordure gris foncé */
    border-radius: 10px;       /* Coins arrondis */
    padding: 10px 20px;        /* Espacement interne */
    font-size: 14px;           /* Taille du texte */
    font-weight: bold;         /* Texte en gras */
}

QPushButton:hover {
    background: none;
    background-color: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1,
        stop:0 #3b3b3b,  /* Gris plus clair au survol */
        stop:0.5 #292929,  
        stop:1 #121212  
    );
}

QPushButton:pressed {
    background: none;
    background-color: #1a1a1a;  /* Noir encore plus profond lors de l'appui */
}

QPushButton:focus {
    outline: none; /* Supprime l'effet de focus (contour) */
}
