  <property name="styleSheet">
   <string notr="true">/* General application styling */
QWidget {
    background-color: #E6F7FB;
    color: #1A2C79;
    font-family: 'Inter', 'Segoe UI', 'Open Sans', sans-serif;
    font-size: 10.5pt;
}

/* Labels */
QLabel {
    color: #1A2C79;
    font-size: 12pt;
    font-weight: 600;
    letter-spacing: 0.3px;
}

/* Line edits (inputs) */
QLineEdit {
    background-color: #FFFFFF;
    border: 1.5px solid #00B4D8;
    border-radius: 6px;
    padding: 5px 8px;
    color: #1A2C79;
    font-size: 10.5pt;
}
QLineEdit:focus {
    border: 1.5px solid #1A2C79;
    box-shadow: 0px 0px 4px rgba(26, 44, 121, 0.3);
}

/* Buttons */
QPushButton {
    background-color: #1A2C79;
    border: none;
    border-radius: 6px;
    padding: 6px 12px;
    font-size: 11pt;
    font-weight: 500;
    color: #FFFFFF;
}
QPushButton:hover {
    background-color: #243D9B;
}
QPushButton:pressed {
    background-color: #12205C;
}

/* Tables */
QTableWidget {
    background-color: #FFFFFF;
    border: 1.5px solid #90E0EF;
    border-radius: 6px;
    gridline-color: #ADE8F4;
    font-size: 10pt;
}
QHeaderView::section {
    background-color: #1A2C79;
    color: white;
    padding: 5px;
    font-weight: bold;
    font-size: 10.5pt;
    border: none;
}

/* Scrollbars */
QScrollBar:vertical {
    background: #D0F0F8;
    width: 10px;
    border-radius: 5px;
}
QScrollBar::handle:vertical {
    background: #00B4D8;
    border-radius: 5px;
}
QScrollBar::handle:vertical:hover {
    background: #009AC2;
}

/* Central widget */
#centralwidget {
    background-color: #B0E9F4;
}

</string>
