  <property name="styleSheet">
   <string notr="true">/* General application styling */
QWidget {
    background-color: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1,
        stop:0 #E0F7FA,
        stop:1 #A7D8F0
    );
    color: #FFFFFF;
    font-family: 'Inter', 'Segoe UI', 'Open Sans', sans-serif;
    font-size: 10.5pt;
}

/* Labels */
QLabel {
    color: #FFFFFF;
    font-size: 12pt;
    font-weight: 600;
    letter-spacing: 0.3px;
}

/* Line edits (inputs) */
QLineEdit {
    background-color: rgba(255, 255, 255, 0.9);
    border: 1.5px solid #00B4D8;
    border-radius: 6px;
    padding: 5px 8px;
    color: #1A2C79;
    font-size: 10.5pt;
}
QLineEdit:focus {
    border: 1.5px solid #1A2C79;
    box-shadow: 0px 0px 5px rgba(26, 44, 121, 0.3);
}

/* Buttons */
QPushButton {
    background-color: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1,
        stop:0 #1A2C79,
        stop:1 #000000
    );
    border: none;
    border-radius: 6px;
    padding: 6px 12px;
    font-size: 11pt;
    font-weight: 500;
    color: #FFFFFF;
}
QPushButton:hover {
    background-color: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1,
        stop:0 #243D9B,
        stop:1 #121212
    );
}
QPushButton:pressed {
    background-color: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1,
        stop:0 #0D1B4C,
        stop:1 #000000
    );
}

/* Tables */
QTableWidget {
    background-color: rgba(255, 255, 255, 0.95);
    border: 1.5px solid #90E0EF;
    border-radius: 6px;
    gridline-color: #ADE8F4;
    font-size: 10pt;
    color: #1A2C79;
}
QHeaderView::section {
    background-color: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1,
        stop:0 #1A2C79,
        stop:1 #000000
    );
    color: white;
    padding: 5px;
    font-weight: bold;
    font-size: 10.5pt;
    border: none;
}

/* Scrollbars */
QScrollBar:vertical {
    background: qlineargradient(
        spread:pad, x1:0, y1:0, x2:0, y2:1,
        stop:0 #A7D8F0,
        stop:1 #E0F7FA
    );
    width: 10px;
    border-radius: 5px;
}
QScrollBar::handle:vertical {
    background: qlineargradient(
        spread:pad, x1:0, y1:0, x2:0, y2:1,
        stop:0 #009AC2,
        stop:1 #005F73
    );
    border-radius: 5px;
}
QScrollBar::handle:vertical:hover {
    background: qlineargradient(
        spread:pad, x1:0, y1:0, x2:0, y2:1,
        stop:0 #0077A0,
        stop:1 #003D4C
    );
}

/* Central widget */
#centralwidget {
    background-color: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1,
        stop:0 #000000,
        stop:1 #1A1A1A
    );
}</string>
